(include "lib/asm/func.inc")
(include "././lisp/class.inc")
(include "./class.inc")
(include "././num/class.inc")
(include "././nums/class.inc")

(def-method 'dim :lisp_dim)
	;inputs
	;:r0 = lisp object (ptr)
	;:r1 = args list object (ptr)
	;outputs
	;:r0 = lisp object (ptr)
	;:r1 = return value object (ptr)
	;trashes
	;:r1-:r14

	(entry 'dim :lisp_dim '(:r0 :r1))

	(errorif-lisp-args-sig 'error :r1 2)

	(vp-push :r0 :r1)
	(bind '(dims elems) (method-input 'dim :create))
	(list-bind-args :r1 `(,dims ,elems) '(:nums :array))

(errorcase
	(assign `((,dims array_length)) '(:r14))
	(gotoif '(:r14 = 0) 'error1))

	(call 'dim :create `(,dims ,elems) '(:r1))
	(vp-pop :r0 :r2)

	(exit 'dim :lisp_dim '(:r0 :r1))
	(vp-ret)

(errorcase
(vp-label 'error1)
	(vp-pop :r0 :r1)
(vp-label 'error)
	(jump 'lisp :repl_error '(:r0 "(dim nums array)" +error_msg_wrong_types :r1))
	(signature '(nums array)))

(def-func-end)

(def-method 'dim :lisp_get)
	;inputs
	;:r0 = lisp object (ptr)
	;:r1 = args list object (ptr)
	;outputs
	;:r0 = lisp object (ptr)
	;:r1 = return value object (ptr)
	;trashes
	;:r1-:r14

	(entry 'dim :lisp_get '(:r0 :r1))

	(errorif-lisp-args-sig 'error :r1 2)

	(vp-def (elems index dims indices dims_len indices_len
			dims_iter_begin dims_iter_end indices_iter_begin acc d i)
		(method-input 'array :ref_elem))

	(vp-push :r0 :r1)
	(list-bind-args :r1 `(,dims ,indices) '(:dim :nums))
	(assign `((,dims dim_dims) (,dims dim_elems)) `(,dims ,elems))
	(assign `((,dims array_length) (,indices array_length)) `(,dims_len ,indices_len))

	(errorif `(,indices_len > ,dims_len) 'error1)

	(assign `((,dims array_begin) (,indices array_begin)) `(,dims_iter_begin ,indices_iter_begin))
	(vp-shl-cr (log2 +ptr_size) indices_len)
	(vp-lea-d dims_iter_begin indices_len dims_iter_end)

	(assign '(0 1) `(,index ,acc))
	(loop-start)
		(vp-simd vp-cpy-ir `(,dims_iter_begin ,indices_iter_begin) '(0) `(,d ,i))
		(vp-simd vp-add-cr `(,+ptr_size) `(,dims_iter_begin ,indices_iter_begin))
		(vp-mul-rr acc i)
		(vp-mul-rr d acc)
		(vp-add-rr i index)
	(loop-until `(,dims_iter_begin = ,dims_iter_end))

(errorcase
	(assign `((,elems array_length)) `(,indices_len))
	(gotoif `(,index < 0) 'error1)
	(gotoif `(,index >= ,indices_len) 'error1))

	(call 'array :ref_elem `(,elems ,index) '(_ :r1))
	(vp-pop :r0 :r2)

	(exit 'dim :lisp_get '(:r0 :r1))
	(vp-ret)

(errorcase
(vp-label 'error1)
	(vp-pop :r0 :r1)
	(jump 'lisp :repl_error '(:r0 "(dim-get dim nums)" +error_msg_not_valid_index :r1))
(vp-label 'error)
	(jump 'lisp :repl_error '(:r0 "(dim-get dim nums)" +error_msg_wrong_types :r1))
	(signature '(dim nums)))

(def-func-end)

(def-method 'dim :lisp_set)
	;inputs
	;:r0 = lisp object (ptr)
	;:r1 = args list object (ptr)
	;outputs
	;:r0 = lisp object (ptr)
	;:r1 = return value object (ptr)
	;trashes
	;:r1-:r14

	(entry 'dim :lisp_set '(:r0 :r1))

	(errorif-lisp-args-sig 'error :r1 3)

	(vp-def (elems obj index dims indices dims_len indices_len
			dims_iter_begin dims_iter_end indices_iter_begin acc d i)
		(method-input 'array :set_elem))

	(vp-push :r0 :r1)
	(list-bind-args :r1 `(,dims ,indices ,obj) '(:dim :nums :obj))
	(assign `((,dims dim_dims) (,dims dim_elems)) `(,dims ,elems))
	(assign `((,dims array_length) (,indices array_length)) `(,dims_len ,indices_len))

	(errorif `(,indices_len > ,dims_len) 'error1)

	(assign `((,dims array_begin) (,indices array_begin)) `(,dims_iter_begin ,indices_iter_begin))
	(vp-shl-cr (log2 +ptr_size) indices_len)
	(vp-lea-d dims_iter_begin indices_len dims_iter_end)

	(assign '(0 1) `(,index ,acc))
	(loop-start)
		(vp-simd vp-cpy-ir `(,dims_iter_begin ,indices_iter_begin) '(0) `(,d ,i))
		(vp-simd vp-add-cr `(,+ptr_size) `(,dims_iter_begin ,indices_iter_begin))
		(vp-mul-rr acc i)
		(vp-mul-rr d acc)
		(vp-add-rr i index)
	(loop-until `(,dims_iter_begin = ,dims_iter_end))

(errorcase
	(assign `((,elems array_length)) `(,indices_len))
	(gotoif `(,index < 0) 'error1)
	(gotoif `(,index >= ,indices_len) 'error1))

	(assign `((,elems obj_vtable) (@ ,(f-path 'list :vtable))) '(:r12 :r13))
	(assign `(,obj) '((:rsp 0)))
	(vpif '(:r12 = :r13))
		(class/obj/ref obj :r14)
	(else)
		(vp-simd vp-cpy-rr `(,elems ,obj ,index) '(:r12 :r13 :r14))
		(call 'obj :inst_of `(,obj (@ ,(f-path 'num :vtable))) '(_ tmp))
		(vpif `(,tmp /= 0))
			(assign '((:r13 num_value)) '(:r13))
		(endif)
		(vp-simd vp-cpy-rr '(:r12 :r13 :r14) `(,elems ,obj ,index))
	(endif)
	(call 'array :set_elem `(,elems ,obj ,index) '(:r1))
	(class/obj/ref :r1 :r2)
	(vp-pop :r0 :r2)

	(exit 'dim :lisp_set '(:r0 :r1))
	(vp-ret)

(errorcase
(vp-label 'error1)
	(vp-pop :r0 :r1)
	(jump 'lisp :repl_error '(:r0 "(dim-set dim nums elem)" +error_msg_not_valid_index :r1))
(vp-label 'error)
	(jump 'lisp :repl_error '(:r0 "(dim-set dim nums elem)" +error_msg_wrong_types :r1))
	(signature '(dim nums obj)))

(def-func-end)
