(def-method 'reals :div)
	;inputs
	;:r0 = reals object (ptr)
	;:r1 = source1 reals object, can be same (ptr)
	;:r2 = source2 reals object, can be same (ptr)
	;outputs
	;:r0 = reals object (ptr)
	;:r1 = 0 if error, else ok
	;trashes
	;:r1-:r4, :f0-:f2

	(vp-def (dst src1 src2 dst1 src_end))
	(vp-fdef (e0 e1 zero))

	(entry 'reals :div `(,dst ,src1 ,src2))

	(vp-cpy-cr 0 src_end)
	(vp-cpy-rf src_end zero)
	(assign `((,src1 array_length)) `(,src_end))
	(assign (map (# (list %0 array_begin)) `(,src1 ,src2 ,dst)) `(,src1 ,src2 ,dst1))
	(vp-shl-cr (log2 +ptr_size) src_end)
	(vp-add-rr src1 src_end)
	(vec-loop-until `(,src1 ,src2) `(,e0 ,e1) src_end dst1
		(lambda (e0 e1)
			(errorif `(,e1 = ,zero) 'error)
			(vp-div-ff e1 e0) e0))

	(exit 'reals :div `(,dst -1))
	(vp-ret)
(vp-label 'error)
	(exit 'reals :div `(,dst 0))
	(vp-ret)

(def-func-end)

(def-method 'reals :mod)
	;inputs
	;:r0 = reals object (ptr)
	;:r1 = source1 reals object, can be same (ptr)
	;:r2 = source2 reals object, can be same (ptr)
	;outputs
	;:r0 = reals object (ptr)
	;:r1 = 0 if error, else ok
	;trashes
	;:r1-:r5, :f0-:f3

	(vp-def (dst src1 src2 dst1 src_end trunc))
	(vp-fdef (e0 e1 e2 zero))

	(entry 'reals :mod `(,dst ,src1 ,src2))

	(vp-cpy-cr 0 src_end)
	(vp-cpy-rf src_end zero)
	(assign `((,src1 array_length)) `(,src_end))
	(assign (map (# (list %0 array_begin)) `(,src1 ,src2 ,dst)) `(,src1 ,src2 ,dst1))
	(vp-shl-cr (log2 +ptr_size) src_end)
	(vp-add-rr src1 src_end)
	(vec-loop-until `(,src1 ,src2) `(,e0 ,e1) src_end dst1
		(lambda (e0 e1)
			(errorif `(,e1 = ,zero) 'error)
			(vp-cpy-ff e0 e2)
			(vp-div-ff e1 e2)
			(vp-cvt-fr e2 trunc)
			(vp-cvt-rf trunc e2)
			(vp-mul-ff e1 e2)
			(vp-sub-ff e2 e0)
			e0))

	(exit 'reals :mod `(,dst -1))
	(vp-ret)
(vp-label 'error)
	(exit 'reals :mod `(,dst 0))
	(vp-ret)

(def-func-end)

(def-method 'reals :frac)
	;inputs
	;:r0 = reals object (ptr)
	;:r1 = source reals object, can be same (ptr)
	;outputs
	;:r0 = reals object (ptr)
	;trashes
	;:r1-:r4, :f0-:f3

	(vp-def (dst src1 dst1 src_end trunc))
	(vp-fdef (e0 e1 one zero))

	(entry 'reals :frac `(,dst ,src1))

	(vp-xor-rr trunc trunc)
	(vp-cvt-rf trunc zero)
	(vp-cpy-cr 1 trunc)
	(vp-cvt-rf trunc one)
	(assign `((,src1 array_length)) `(,src_end))
	(assign (map (# (list %0 array_begin)) `(,src1 ,dst)) `(,src1 ,dst1))
	(vp-shl-cr (log2 +ptr_size) src_end)
	(vp-add-rr src1 src_end)
	(vec-loop-until `(,src1) `(,e0) src_end dst1
		(lambda (e0)
			(vp-cvt-fr e0 trunc)
			(vp-cvt-rf trunc e1)
			(vp-sub-ff e1 e0)
			(vpif `(,e0 > ,zero))
				(vp-add-ff one e0)
			(endif)
			e0))

	(exit 'reals :frac `(,dst))
	(vp-ret)

(def-func-end)

