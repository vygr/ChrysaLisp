(def-method 'reals :div)
	;inputs
	;:r0 = reals object (ptr)
	;:r1 = source1 reals object, can be same (ptr)
	;:r2 = source2 reals object, can be same (ptr)
	;outputs
	;:r0 = reals object (ptr)
	;:r1 = 0 if error, else ok
	;trashes
	;:r1-:r4, :f0-:f2

	(vp-rdef (dst src1 src2 dst1 src_end))
	(vp-fdef (e0 e1 zero))

	(entry 'reals :div `(,dst ,src1 ,src2))

	(vec-map (list src1 src2) (list dst) (list e0 e1)
		(list src1 src2) (list dst1) src_end
		(#  (errorif `(,%%1 = ,zero) 'error)
			(vp-div-ff %1 %0) %0)
		(# 	(vp-sub-ff zero zero)))

	(exit 'reals :div `(,dst -1))
	(vp-ret)
(vp-label 'error)
	(exit 'reals :div `(,dst 0))
	(vp-ret)

(def-func-end)

(def-method 'reals :mod)
	;inputs
	;:r0 = reals object (ptr)
	;:r1 = source1 reals object, can be same (ptr)
	;:r2 = source2 reals object, can be same (ptr)
	;outputs
	;:r0 = reals object (ptr)
	;:r1 = 0 if error, else ok
	;trashes
	;:r1-:r5, :f0-:f3

	(vp-rdef (dst src1 src2 dst1 src_end trunc))
	(vp-fdef (e0 e1 ftrunc zero))

	(entry 'reals :mod `(,dst ,src1 ,src2))

	(vec-map (list src1 src2) (list dst) (list e0 e1)
		(list src1 src2) (list dst1) src_end
		(#  (errorif `(,%%1 = ,zero) 'error)
			(vp-cpy-ff %0 ftrunc)
			(vp-div-ff %1 ftrunc)
			(vp-cvt-fr ftrunc trunc)
			(vp-cvt-rf trunc ftrunc)
			(vp-mul-ff %1 ftrunc)
			(vp-sub-ff ftrunc %0)
			%0)
		(# 	(vp-sub-ff zero zero)))

	(exit 'reals :mod `(,dst -1))
	(vp-ret)
(vp-label 'error)
	(exit 'reals :mod `(,dst 0))
	(vp-ret)

(def-func-end)

(def-method 'reals :frac)
	;inputs
	;:r0 = reals object (ptr)
	;:r1 = source reals object, can be same (ptr)
	;outputs
	;:r0 = reals object (ptr)
	;trashes
	;:r1-:r4, :f0-:f3

	(vp-rdef (dst src1 dst1 src_end trunc))
	(vp-fdef (e0 ftrunc one zero))

	(entry 'reals :frac `(,dst ,src1))

	(vec-map (list src1) (list dst) (list e0)
		(list src1) (list dst1) src_end
		(#  (vp-cvt-fr %0 trunc)
			(vp-cvt-rf trunc ftrunc)
			(vp-sub-ff ftrunc %0)
			(vpif `(,%%0 < ,zero))
				(vp-add-ff one %0)
			(endif)
			%0)
		(# (vp-sub-ff zero zero)
			(vp-cpy-cr 1 trunc)
			(vp-cvt-rf trunc one)))

	(exit 'reals :frac `(,dst))
	(vp-ret)

(def-func-end)

(def-method 'reals :floor)
	;inputs
	;:r0 = reals object (ptr)
	;:r1 = source reals object, can be same (ptr)
	;outputs
	;:r0 = reals object (ptr)
	;trashes
	;:r1-:r4, :f0-:f2

	(vp-rdef (dst src1 dst1 src_end trunc))
	(vp-fdef (e0 ftrunc one))

	(entry 'reals :floor `(,dst ,src1))

	(vec-map (list src1) (list dst) (list e0)
		(list src1) (list dst1) src_end
		(#  (vp-cvt-fr %0 trunc)
			(vp-cvt-rf trunc ftrunc)
			(vpif `(,ftrunc > ,%%0))
				(vp-sub-ff one ftrunc)
			(endif)
			ftrunc)
		(# 	(vp-cpy-cr 1 trunc)
			(vp-cvt-rf trunc one)))

	(exit 'reals :floor `(,dst))
	(vp-ret)

(def-func-end)

