(def-method 'reals :sum)
	;inputs
	;:r0 = reals object (ptr)
	;outputs
	;:r0 = reals object (ptr)
	;:r1 = sum (real)
	;trashes
	;:r1-:r2, :f0-:f1

	(vp-def (this src1 src_end))
	(vp-fdef (sum val))

	(entry 'reals :sum `(,this))

	(vp-xor-rr src_end src_end)
	(vp-cvt-rf src_end sum)
	(class/array/get_both this src1 src_end)
	(vpif `(,src1 /= ,src_end))
		(assign `((,src1 0)) `(,sum))
		(vp-add-cr +long_size src1)
		(loop-while `(,src1 /= ,src_end))
			(assign `((,src1 0)) `(,val))
			(vp-add-ff val sum)
			(vp-add-cr +long_size src1)
		(loop-end)
	(endif)

	(exit 'reals :sum `(,this ,sum))
	(vp-ret)

(def-func-end)

(def-method 'reals :dot)
	;inputs
	;:r0 = reals object (ptr)
	;:r1 = reals object, can be same (ptr)
	;outputs
	;:r0 = reals object (ptr)
	;:r1 = dot product (real)
	;trashes
	;:r1-:r14

	(vp-def (this src2 src1 src_end))
	(vp-fdef (sum val1 val2))

	(entry 'reals :dot `(,this ,src2))

	(vp-xor-rr src_end src_end)
	(vp-cvt-rf src_end sum)
	(class/array/get_both this src1 src_end)
	(assign `((,src2 array_begin)) `(,src2))
	(vpif `(,src1 /= ,src_end))
		(assign `((,src1 0) (,src2 0)) `(,val1 ,sum))
		(vp-add-cr +long_size src1)
		(vp-add-cr +long_size src2)
		(vp-mul-ff val1 sum)
		(loop-while `(,src1 /= ,src_end))
			(assign `((,src1 0) (,src2 0)) `(,val1 ,val2))
			(vp-add-cr +long_size src1)
			(vp-add-cr +long_size src2)
			(vp-mul-ff val1 val2)
			(vp-add-ff val2 sum)
		(loop-end)
	(endif)

	(exit 'reals :dot `(,this ,sum))
	(vp-ret)

(def-func-end)
