(gen-create 'reals)
(gen-vtable 'reals)
(gen-type 'reals)

(def-method 'reals :add)
	;inputs
	;:r0 = reals object (ptr)
	;:r1 = source1 reals object, can be same (ptr)
	;:r2 = source2 reals object, can be same (ptr)
	;outputs
	;:r0 = reals object (ptr)
	;:r1 = 0 if error, else ok
	;trashes
	;:r1-:r4, :f0-:f1

	(vp-rdef (dst src1 src2 dst1 src_end))
	(vp-fdef (e0 e1))

	(entry 'reals :add `(,dst ,src1 ,src2))

	(vec-map (list src1 src2) (list dst) (list e0 e1)
		(list src1 src2) (list dst1) src_end
		(# (vp-add-ff %1 %0) %0))

	(exit 'reals :add `(,dst -1))
	(vp-ret)

(def-func-end)

