	;trashes
	;:r1-:r14, :f0-:f1

	(minop :min
		'(progn
			(vpif `(,new_val < ,val))
				(vp-cpy-ff new_val val)
				(vp-cpy-rr obj args)
			(endif)))

(def-func-end)

