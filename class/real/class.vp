(def-method :real :random)
	;inputs
	;:r0 = real object (ptr)
	;outputs
	;:r0 = result real object (ptr)
	;trashes
	;:r0-:r14, :f0-:f1

	(mathop :random
		'(progn
			(vp-lea-p 'fn_consts :r4)
			(assign '((:r4 (fn-const (n2r 1)))) '(:f0))
			(vp-mul-ff :f0 :f1)
			(vp-cvt-fr :f1 :r0)
			(call :sys_math :i_rand '(:r0) '(:r0))
			(vp-cvt-rf :r0 :f0)
			(assign '((:r4 (fn-const (recip (n2r 1))))) '(:f1))
			(vp-mul-ff :f1 :f0)))

(def-func-end)

(def-method :real :quant)
	;inputs
	;:r0 = real object (ptr)
	;:r1 = tolerance real object (ptr)
	;outputs
	;:r0 = result real object (ptr)
	;trashes
	;:r0-:r14, :f0-:f3

	(vp-rdef (this tol trunc))
	(vp-fdef (v tmp half one ftrunc))

	(entry :real :quant `(,this ,tol))

	(assign `((,this num_value) (,tol num_value)) `(,v ,tmp))

	; val / tol
	(vp-div-ff tmp v)
	(vp-lea-p 'fn_consts tol)

	; floor(v)
	(vp-cvt-fr v trunc)
	(vp-cvt-rf trunc ftrunc)
	(vpif `(,ftrunc > ,v))
		(assign `((,tol (fn-const (n2r 1.0)))) `(,one))
		(vp-sub-ff one ftrunc)
	(endif)
	(vp-cpy-ff ftrunc v)

	; + 0.5
	(assign `((,tol (fn-const (n2r 0.5)))) `(,half))
	(vp-add-ff half v)

	; * tol
	(vp-mul-ff tmp v)
	(jump :real :create `(,v))

(def-func-end)
