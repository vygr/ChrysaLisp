(def-method :real :frac)
	;inputs
	;:r0 = real object (ptr)
	;outputs
	;:r0 = result real object (ptr)
	;trashes
	;:r0-:r14, :f0-:f1

	(mathop :frac
		'(progn
			(vp-lea-p 'fn_consts :r1)
			(vp-cpy-ff :f1 :f0)
			(vp-cvt-fr :f0 :r0)
			(vp-cvt-rf :r0 :f1)
			(vp-sub-ff :f1 :f0)
			(assign '((:r1 (fn-const (n2r 0)))) '(:f1))
			(vpif '(:f0 < :f1))
				(assign '((:r1 (fn-const (n2r 1)))) '(:f1))
				(vp-add-ff :f1 :f0)
			(endif)))

(def-func-end)

