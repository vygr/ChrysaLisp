(redefun divop (%0 p)
	(entry :real %0 '(:r0 :r1))
	(class/array/get_both :r1 :r1 :r4)
	(vp-cpy-ir :r1 0 :r0)
	(vp-add-cr +ptr_size :r1)
	(assign '((:r0 num_value) 0) '(:f0 :r3))
	(vp-cpy-rf :r3 :f3)
	(loop-start)
		(vp-cpy-ir :r1 0 :r5)
		(vp-add-cr +ptr_size :r1)
		(assign '((:r5 num_value)) '(:f1))
	(errorcase
		(gotoif '(:f1 = :f3) 'error))
		(exec p)
	(loop-until '(:r1 = :r4))
	(v-jump :num :vcreate '(:f0) :r5 :r1)
(vp-label 'error)
	(exit :real %0 '(0))
	(vp-ret))

(def-method :real :div)
	;inputs
	;:r0 = real object (ptr)
	;:r1 = list of real objects (ptr)
	;outputs
	;:r0 = 0 if error, else result real object (ptr)
	;trashes
	;:r1-:r14, :f0-:f3

	(divop :div
		'(vp-div-ff :f1 :f0))

(def-func-end)

(def-method :real :mod)
	;inputs
	;:r0 = real object (ptr)
	;:r1 = list of real objects (ptr)
	;outputs
	;:r0 = 0 if error, else result real object (ptr)
	;trashes
	;:r1-:r14, :f0-:f3

	(divop :mod
		'(progn
			(vp-cpy-ff :f0 :f2)		; Copy dividend (x)
			(vp-div-ff :f1 :f2)		; f2 = x / y
			(vp-cvt-fr :f2 :r0)		; r0 = (int)(x / y) [Truncate]
			(vp-cvt-rf :r0 :f2)		; f2 = (double)(int)(x / y)
			(vp-mul-ff :f1 :f2)		; f2 = y * trunc(x / y)
			(vp-sub-ff :f2 :f0)))	; f0 = x - (y * trunc(x / y))

(def-func-end)

(redefun compareop (%0 p)
	(entry :real %0 '(:r0 :r1))
	(class/array/get_both :r1 :r1 :r2)
	(vp-cpy-ir :r1 0 :r3)
	(vp-add-cr +ptr_size :r1)
	(assign '((:r3 num_value)) '(:f0))
	(loop-start)
		(vp-cpy-ir :r1 0 :r3)
		(vp-add-cr +ptr_size :r1)
		(assign '((:r3 num_value)) '(:f1))
		(exec p)
	(loop-until '(:r1 = :r2))
	(exit :real %0 '(-1))
	(vp-ret)
(vp-label 'fail)
	(exit :real %0 '(0))
	(vp-ret))

