(def-method :real :print)
	;inputs
	;:r0 = real object (ptr)
	;:r1 = stream object (ptr)
	;outputs
	;:r0 = real object (ptr)
	;trashes
	;:r1-:r14

	(def-vars
		(ptr this stream tmp))

	(entry :real :print '(:r0 :r1))

	(push-scope)
	(assign '(:r0 :r1) {this, stream})
	(call :stream :write_cstr '(:r1 "0x"))
	(call :str :create_from_long {this->num_value, -16} {tmp})
	(call :stream :write {stream, &tmp->str_data, tmp->str_length})
	(call :obj :deref {tmp})

	(exit :real :print {this})
	(pop-scope)
	(return)

(def-func-end)

(redefun addop (%0)
	(entry :real %0 '(:r0 :r1))
	(class/array/get_both :r1 :r1 :r2)
	(vp-cpy-ir :r1 0 :r0)
	(vp-add-cr +ptr_size :r1)
	(assign '((:r0 num_value)) '(:f0))
	(loop-start)
		(vp-cpy-ir :r1 0 :r4)
		(vp-add-cr +ptr_size :r1)
		(assign '((:r4 num_value)) '(:f1))
		((eval (sym (cat "vp-" (rest %0) "-ff"))) :f1 :f0)
	(loop-until '(:r1 = :r2))
	(v-jump :num :vcreate '(:f0) :r4 :r1))

