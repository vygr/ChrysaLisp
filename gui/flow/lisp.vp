(include 'sys/func.inc)
(include 'gui/flow/class.inc)
(include 'class/lisp/class.inc)

(def-method 'flow 'lisp_create)
	;inputs
	;r0 = lisp object (ptr)
	;r1 = args vector object (ptr)
	;outputs
	;r0 = lisp object (ptr)
	;r1 = return value object (ptr)
	;trashes
	;all but r0

	(entry 'flow 'lisp_create '(r0 r1))

(errorcases
	(vp-cpy-ir-ui r1 array_length r2)
	(gotoif '(r2 != 0) 'error))

	(vp-push r0)
	(call 'flow 'create nil '(r1))
	(vp-pop r0)

	(exit 'flow 'lisp_create '(r0 r1))
	(vp-ret)

(errorcases
(vp-label 'error)
	(jump 'lisp 'repl_error '(r0 "(create-flow)" error_msg_wrong_num_of_args r1)))

(def-func-end)
