(def-method 'path :lisp_svg)
	;inputs
	;r0 = lisp object (ptr)
	;r1 = args list object (ptr)
	;outputs
	;r0 = lisp object (ptr)
	;r1 = return value object (ptr)
	;trashes
	;r1-r14

	(def-vars
		(ptr this args commands command_chars num_start_chars num_mid_chars)
		(pubyte d d_start)
		(int state c))

	(entry 'path :lisp_svg '(r0 r1))

(errorcases
	(call 'lisp :env_args_sig '(r1 ($ sig) 1) '(r2))
	(gotoif '(r2 = 0) 'error))

	(push-scope)
	(assign '(r0 r1) {this, args})

	(list-bind-args r1 '(r0) '(:str))
	(vp-xor-rr r1 r1)
	(assign '(r0 r1) {d, state})
	(call 'list :create nil {commands})
	(call 'str :create_from_buffer {"MLHVCSQTAZmlhvcsqtaz", 20} {command_chars})
	(call 'str :create_from_buffer {"-0123456789", 11} {num_start_chars})
	(call 'str :create_from_buffer {".0123456789", 11} {num_mid_chars})

	(defq str_find_in (method-input 'str :find)
		str_find_out (method-output 'str :find)
		str_to_num_out (method-output 'sys_str :to_num)
		push_back_in (method-input 'list :push_back))

	(loop-while {(*d) => c})
		(vpifnot {state} 'next_char)
		(vp-label 'eat_char)
			(assign {num_start_chars, c} str_find_in)
			(breakif (list (elem 1 str_find_in) '<= +char_space))
			(breakif (list (elem 1 str_find_in) '= +char_comma))
			(d-call 'str :find str_find_in str_find_out)
			(vpif (list (elem 2 str_find_out) '/= -1))
				;number
				(assign {d, 1} {d_start, state})
				(break 'next_char)
			(endif)
		(errorcases
			(assign {command_chars, c} str_find_in)
			(d-call 'str :find str_find_in str_find_out)
			(gotoif (list (elem 2 str_find_out) '= -1) 'error1))
			;command
			(call 'str :create_from_buffer {&c, 1} (list (elem 1 push_back_in)))
			(assign {commands} (list (elem 0 push_back_in)))
			(call 'list :push_back push_back_in)
		(else)
			(assign {num_mid_chars, c} str_find_in)
			(d-call 'str :find str_find_in str_find_out)
			(breakif (list (elem 2 str_find_out) '/= -1))
			(assign {0} {*d})
			(call 'sys_str :to_num {d_start} str_to_num_out)
			(vpif (list (elem 1 str_to_num_out) '= 0))
				(vp-shl-cr +fp_shift (elem 0 str_to_num_out))
			(endif)
			(call 'fixed :create (list (elem 0 str_to_num_out)) (list (elem 1 push_back_in)))
			(assign {commands} (list (elem 0 push_back_in)))
			(call 'list :push_back push_back_in)
			(assign {c, 0} {*d, state})
			(goto 'eat_char)
		(endif)
		(assign {d + 1} {d})
	(loop-end)
	(call 'str :create_from_buffer '("x" 1) (list (elem 1 push_back_in)))
	(assign {commands} (list (elem 0 push_back_in)))
	(call 'list :push_back push_back_in)

	(call 'str :destroy {command_chars})
	(call 'str :destroy {num_start_chars})
	(call 'str :destroy {num_mid_chars})

	(exit 'path :lisp_svg {this, commands})
	(return)

(noterrorcases
	(pop-scope-syms))

(errorcases
(vp-label 'error1)
	(call 'str :destroy {commands})
	(call 'str :destroy {command_chars})
	(call 'str :destroy {num_start_chars})
	(call 'str :destroy {num_mid_chars})
	(assign {this, args} '(r0 r1))
	(pop-scope)
(vp-label 'error)
	(jump 'lisp :repl_error '(r0 "(path-svg d) -> commands" error_msg_wrong_types r1))
	(signature '(str)))

(def-func-end)
